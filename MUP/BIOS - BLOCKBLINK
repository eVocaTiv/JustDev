.model tiny
.DATA
fname db 'det.txt' , 0
nfname db 'avinash.txt' , 0
handle dw ?
sid db 'a' , 'b' , 'c'
line db 0dh , 0ah
id db 3 dup(?)
.CODE
.STARTUP





	;SET DISPLAY MODE
	MOV AH , 00H
	MOV AL , 03H
	INT 10H

	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 10
	MOV DL , 30
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 11
	MOV DL , 32
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 12
	MOV DL , 34
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 13
	MOV DL , 36
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 14
	MOV DL , 34
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 15
	MOV DL , 32
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	;SET CURSOR POSITION
	MOV AH , 02H
	MOV DH , 16
	MOV DL , 30
	MOV BH , 00H
	INT 10H
	
	;WRITE CHAR AT CURSOR POSITION
	MOV AH , 09H
	MOV AL , 'A'
	MOV BH , 00H
	MOV BL , 10001001B
	MOV CX , 0AH
	INT 10H
	
	
	
	;BLOCKING FUNCTION WITH %
	MOV AH , 07H
BLOCK:
	INT 21H
	CMP AL , '%'
	JNZ BLOCK

	
	
	

.EXIT
END
